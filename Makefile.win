# Project: SeewoKiller
# Makefile created by Red Panda C++ 3.4.3279

CXX      = "g++.exe"
CC       = "gcc.exe"
WINDRES  = "windres.exe"
RM       = del /q /f
CD       = cd /d
LIBS     = "-Wl,--stack,12582912" "-s" "-lShell32" "-lOle32" "-static"
INCS     = 
CXXINCS  = 
CXXFLAGS = $(CXXINCS) "-O2" "-pipe"
CFLAGS   = $(INCS) "-O2" "-pipe"
NASM_FLAGS   =  "-f" "elf64"
WINDRESFLAGS = 
RES      = SeewoKiller_private.res
OBJ      = main.o game.o cmdCtrl.o files.o $(RES)
BIN      = SeewoKiller.exe
LINKOBJ  = "main.o" "game.o" "cmdCtrl.o" "files.o" "SeewoKiller_private.res"
CLEANOBJ = "main.o" "game.o" "cmdCtrl.o" "files.o" "SeewoKiller_private.res" "SeewoKiller.exe"

.PHONY: all all-before all-after clean clean-custom

all: all-before $(BIN) all-after

clean: clean-custom
	-$(RM) $(CLEANOBJ) >NUL 2>&1

SeewoKiller.exe: $(OBJ)

	$(CXX) $(LINKOBJ) -o "SeewoKiller.exe" $(LIBS)

main.o: main.cpp files.h game.h main.h cmdCtrl.h
	$(CXX) -c "main.cpp" -o "main.o" $(CXXFLAGS) 

game.o: game.cpp game.h main.h cmdCtrl.h
	$(CXX) -c "game.cpp" -o "game.o" $(CXXFLAGS) 

cmdCtrl.o: cmdCtrl.cpp cmdCtrl.h
	$(CXX) -c "cmdCtrl.cpp" -o "cmdCtrl.o" $(CXXFLAGS) 

files.o: files.cpp files.h main.h
	$(CXX) -c "files.cpp" -o "files.o" $(CXXFLAGS) 

SeewoKiller_private.res: SeewoKiller_private.rc 
	$(WINDRES) -i "SeewoKiller_private.rc" --input-format=rc -o "SeewoKiller_private.res" -O coff $(WINDRESFLAGS) 

